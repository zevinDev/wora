{
  "version": 2,
  "builds": [
    {
      "src": "renderer/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/lastfm/(.*)",
      "dest": "renderer/pages/api/lastfm/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "renderer/pages/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "renderer/$1"
    }
  ],
  "env": {
    "VERCEL": "true"
  }
}